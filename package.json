{
  "name": "@alauda/ui",
  "version": "6.1.5",
  "description": "Angular UI components by Alauda Frontend Team.",
  "repository": "git+https://github.com/alauda/alauda-ui.git",
  "author": "Alauda Frontend",
  "contributors": [
    "FengTianze",
    "JounQin <admin@1stG.me> (https://www.1stG.me)"
  ],
  "license": "MIT",
  "keywords": [
    "alauda",
    "angular",
    "components",
    "ui"
  ],
  "scripts": {
    "build": "run-s build:lib build:copy",
    "build:copy": "node scripts/copy-resources",
    "build:lib": "ng-packagr -c tsconfig.lib.json",
    "debug": "yarn build:lib -p ng-package.debug.json && yarn build:copy --debug",
    "dev": "yarn start",
    "lint": "run-p lint:*",
    "lint:es": "cross-env PARSER_NO_WATCH=true eslint . --cache --ext js,md,mdx,ts -f friendly",
    "lint:style": "stylelint --cache '{src,stories}/**/*.{html,scss,ts}'",
    "lint:ts": "tslint -p . -t stylish",
    "lint:tsc": "tsc -p tsconfig.base.json --noEmit",
    "postinstall": "yarn-deduplicate --strategy fewer || exit 0",
    "prerelease": "yarn build",
    "release": "changeset publish",
    "start": "yarn storybook",
    "storybook": "start-storybook -c .storybook -p 9001",
    "storybook:build": "build-storybook -c .storybook -o dist",
    "test": "jest --coverage --detectOpenHandles",
    "test:ci": "cross-env JEST_JUNIT_OUTPUT_DIR=reports jest --runInBand --coverage --reporters=jest-junit",
    "test:clear": "jest --clearCache",
    "test:update": "jest -u",
    "test:watch": "jest --watch"
  },
  "peerDependencies": {
    "@angular/animations": ">=11.0.0",
    "@angular/cdk": ">=11.2.0",
    "@angular/common": ">=11.0.0",
    "@angular/core": ">=11.0.0",
    "@angular/forms": ">=11.0.0",
    "@angular/platform-browser": ">=11.0.0",
    "clipboard-polyfill": "^3.0.0",
    "dayjs": "^1.0.0",
    "rxjs": "^6.0.0"
  },
  "devDependencies": {
    "@1stg/app-config": "^1.2.9",
    "@1stg/tslint-config": "^1.1.0",
    "@angular-devkit/build-angular": "^0.1102.3",
    "@angular-devkit/core": "^11.2.3",
    "@angular/animations": "^11.2.4",
    "@angular/cdk": "^11.2.3",
    "@angular/cli": "^11.2.3",
    "@angular/common": "^11.2.4",
    "@angular/compiler": "^11.2.4",
    "@angular/compiler-cli": "^11.2.4",
    "@angular/core": "^11.2.4",
    "@angular/forms": "^11.2.4",
    "@angular/platform-browser": "^11.2.4",
    "@angular/platform-browser-dynamic": "^11.2.4",
    "@changesets/changelog-github": "^0.4.0",
    "@changesets/cli": "^2.16.0",
    "@storybook/addon-actions": "^6.1.21",
    "@storybook/addon-docs": "^6.1.21",
    "@storybook/addon-knobs": "^6.1.21",
    "@storybook/addon-storysource": "^6.1.21",
    "@storybook/addons": "^6.1.21",
    "@storybook/angular": "^6.1.21",
    "@storybook/cli": "^6.1.21",
    "@storybook/source-loader": "^6.1.21",
    "@types/chroma-js": "^2.1.3",
    "@types/file-saver": "^2.0.5",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.31",
    "@types/resize-observer-browser": "^0.1.5",
    "chroma-js": "^2.1.2",
    "clipboard-polyfill": "^3.0.2",
    "dayjs": "^1.10.4",
    "file-saver": "^2.0.5",
    "gulp": "^4.0.2",
    "gulp-dart-sass": "^1.0.2",
    "jest": "^26.6.3",
    "jest-junit": "^12.0.0",
    "jest-preset-angular": "^8.4.0",
    "ng-packagr": "^11.2.4",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.2.7",
    "rxjs": "^6.6.6",
    "storybook-dark-mode": "^1.0.4",
    "ts-node": "^9.1.1",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "tslib": "^2.1.0",
    "tslint": "^6.1.3",
    "webpack-merge": "^5.7.3",
    "yarn-deduplicate": "^3.1.0"
  },
  "resolutions": {
    "prettier": "^2.2.1"
  },
  "publishConfig": {
    "access": "public",
    "directory": "release"
  }
}
