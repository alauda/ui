{
  "name": "@alauda/ui",
  "version": "5.3.2",
  "description": "Angular UI components by Alauda Frontend Team.",
  "repository": "git+https://github.com/alauda/alauda-ui.git",
  "author": "Alauda Frontend",
  "contributors": [
    "FengTianze",
    "JounQin <admin@1stG.me> (https://www.1stG.me)"
  ],
  "license": "MIT",
  "keywords": [
    "alauda",
    "angular",
    "components",
    "ui"
  ],
  "scripts": {
    "build": "ng-packagr && yarn build:copy",
    "build:copy": "node scripts/copy-resources",
    "lint": "run-p lint:*",
    "lint:es": "cross-env PARSER_NO_WATCH=true eslint . --cache --ext js,md,mdx,ts -f friendly",
    "lint:style": "stylelint --cache '{src,stories}/**/*.scss'",
    "lint:ts": "tslint -p . -t stylish",
    "lint:tsc": "tsc --incremental false --noEmit",
    "postinstall": "yarn-deduplicate || exit 0",
    "release": "standard-version --no-verify --release-as",
    "start": "yarn storybook",
    "storybook": "start-storybook -c .storybook -p 9001",
    "storybook:build": "build-storybook -c .storybook -o dist",
    "test": "jest --coverage",
    "test:ci": "cross-env JEST_JUNIT_OUTPUT_DIR=reports jest --runInBand --coverage --reporters=jest-junit",
    "test:clear": "jest --clearCache",
    "test:update": "jest -u",
    "test:watch": "jest --watch"
  },
  "peerDependencies": {
    "@angular/animations": "^10.0.0",
    "@angular/cdk": "^10.2.2",
    "@angular/common": "^10.0.0",
    "@angular/core": "^10.0.0",
    "@angular/forms": "^10.0.0",
    "@angular/platform-browser": "^10.0.0",
    "clipboard-polyfill": "^3.0.0",
    "rxjs": "^6.0.0"
  },
  "devDependencies": {
    "@1stg/app-config": "^0.5.5",
    "@1stg/lib-config": "^0.5.5",
    "@1stg/tslint-config": "^0.8.5",
    "@angular-devkit/build-angular": "^0.1001.2",
    "@angular-devkit/core": "^10.1.2",
    "@angular/animations": "^10.1.2",
    "@angular/cdk": "^10.2.2",
    "@angular/cli": "^10.1.2",
    "@angular/common": "^10.1.2",
    "@angular/compiler": "^10.1.2",
    "@angular/compiler-cli": "^10.1.2",
    "@angular/core": "^10.1.2",
    "@angular/forms": "^10.1.2",
    "@angular/platform-browser": "^10.1.2",
    "@angular/platform-browser-dynamic": "^10.1.2",
    "@storybook/addon-actions": "^6.0.21",
    "@storybook/addon-docs": "^6.0.21",
    "@storybook/addon-knobs": "^6.0.21",
    "@storybook/addon-storysource": "^6.0.21",
    "@storybook/addons": "^6.0.21",
    "@storybook/angular": "^6.0.21",
    "@storybook/cli": "^6.0.21",
    "@storybook/source-loader": "^6.0.21",
    "@types/jest": "^26.0.14",
    "@types/node": "^14.11.2",
    "babel-loader": "^8.1.0",
    "clipboard-polyfill": "^3.0.1",
    "gulp": "^4.0.2",
    "gulp-dart-sass": "^1.0.2",
    "jest": "^26.4.2",
    "jest-junit": "^11.1.0",
    "jest-preset-angular": "^8.3.1",
    "ng-packagr": "^10.1.0",
    "ng-tslint": "^1.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.1.2",
    "rxjs": "^6.6.2",
    "standard-version": "^9.0.0",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "tslib": "^2.0.1",
    "tslint": "^6.1.3",
    "typescript": "^4.0.3",
    "yarn-deduplicate": "^3.0.0"
  }
}
